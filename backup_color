 {
    'projekt0n/github-nvim-theme',
    name = 'github-theme',
    lazy = false,
    priority = 1000,
    config = function()
      require('github-theme').setup {
        palettes = {},
        specs = { all = { bg0 = '#ffffff', bg3 = '#f5f6f8' } },
      }
      vim.cmd.colorscheme 'github_light_default'
    end,
  },
